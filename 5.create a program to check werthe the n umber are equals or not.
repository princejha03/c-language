#include<stdio.h>
int main()
{
    int a,b;
    printf("enter the 1st number= ");
    scanf("%d",&a);
    printf("enter the 2st number= ");
    scanf("%d",&b);
    if(a==b){
        printf("both numbers %d and %d are equal");
    }
    else{
        
        printf("both numbers %d and %d are not equal");
    }
    return 0;
}
