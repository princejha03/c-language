#include<stdio.h>
int revedigit(int num){
    int rev = 0;
    while(num>0){
    rev = rev*10 + num%10;
    num = num/10;
    }
    return rev;
}
int main(){
    int num;
    printf("Enter the number: ");
    scanf("%d",&num);
    printf("The real number\n %d\n The reverse number\n %d\n",num,revedigit(num));
    return 0;
    
}
